%div.jumbotron
  %h1.display-4 Recherche par date
  = form_for :res, method: 'get', url: results_path do |f|
    %p
      = f.label :start_date, "Arrivée"
      %br/
      = f.text_field :start_date, class: 'datepicker'
    %p
      = f.label :end_date, "Départ"
      %br/
      = f.text_field :end_date, class: 'datepicker'
    %p
      = f.submit "Rechercher"
      %br/
  = link_to 'Retour', admin_reservations_path
