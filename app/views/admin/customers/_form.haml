%div#newForm
  = form_for(:customer, url: admin_customers_path) do |f|
    %p
      = f.label :first_name
      %br/
      = f.text_field :first_name
    %p
      = f.label :last_name
      %br/
      = f.text_field :last_name
    %p
      = f.label :email
      %br/
      = f.text_field :email

    %p
      = f.submit
      %br/