%div#home-bg
  %div#date
    %div#date-content
      = form_for :reservation, method: 'get', url: results_path do |f|
        %p
          = f.label :start_date, "Arrivée"
          %br/
          = f.text_field :start_date, class: 'datepicker'
        %p
          = f.label :end_date, "Départ"
          %br/
          = f.text_field :end_date, class: 'datepicker'
        %p
          = f.submit "Rechercher"

